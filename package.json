    "eslint-plugin-jsdoc": "^46.8.2",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.6.4",
    "lint-staged": "^16.1.0",
    "prettier": "^3.0.2",
    "prettier-plugin-jsdoc": "^1.1.1",
    "readme-md-generator": "^0.2.1",
    "rimraf": "^5.0.1",
