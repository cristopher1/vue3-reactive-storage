  "repository": {
    "url": "https://github.com/cristopher1/vue3-reactive-storage"
  },
  "keywords": [
    "Vue3",
    "plugin",
    "reactivity",
    "Storage",
    "LocalStorage",
    "SessionStorage",
    "localStorage",
    "sessionStorage"
