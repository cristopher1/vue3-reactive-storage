  "description": "Vue3 plugin to use reactivity with object that implements the Storage interface",
  "main": "./dist/esm/index.mjs",
  "type": "module",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "types": "./dist/types/index.d.ts",
    "import": "./dist/esm/index.mjs"
  },
  "files": [
    "dist"
  ],
  "author": {
    "name": "Cristopher Jim√©nez Meza",
    "email": "",
